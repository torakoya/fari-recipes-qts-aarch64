pkgname=git-manpages
pkgver=2.40.0
arcext=tar.xz
arcurlbase=https://mirrors.edge.kernel.org/pub/software/scm/git/

srcdir=$workdir

_config () {
    :
}

_build () {
    :
}

_test () {
    :
}

_install () {
    fr_do mkdir -p \"$DESTDIR/$prefix\"/share/man &&
    fr_do cp -R man* \"$DESTDIR/$prefix\"/share/man
}
