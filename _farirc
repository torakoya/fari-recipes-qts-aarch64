# Avoid unexpected behavior
export LC_ALL=C

# QTS's /tmp is too small
export TMPDIR="$HOME"/tmp

#archome=
rechome=~/doc/fari-recipes-qts-aarch64
prefix=/usr/pkg

# nonempty strings mean yes, an empty string means no
install_strip=1
install_gzip=1

# User-defined variables; used in recipes
optflags='-O2 -mcpu=native'
