pkgname=man-pages
pkgver=6.10
arcext=tar.xz
arcurlbase=https://mirrors.edge.kernel.org/pub/linux/docs/man-pages/

_patch () {
    # crypt.3 は libxcrypt のものを採用する。
    fr_do mkdir off-man3 &&
    fr_do mv man3/crypt* off-man3
}

_config () {
    :
}

_build () {
    :
}

_test () {
    :
}

_install () {
    make install prefix="$prefix" "$@"
}
